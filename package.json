{
  "name": "voice-plant-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "esbuild src/main.ts --bundle --outfile=dist/bundle.js --format=esm",
    "dev": "esbuild src/main.ts --bundle --outfile=dist/bundle.js --format=esm --watch",
    "serve": "python3 -m http.server 8000",
    "serve:public": "cd public && python3 -m http.server 8001"
  },
  "devDependencies": {
    "@types/p5": "^1.7.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "p5": "^1.7.0",
    "simplex-noise": "^4.0.1"
  }
}
